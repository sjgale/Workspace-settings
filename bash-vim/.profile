# Personal aliases
alias me="cd ~"
alias editbash="code ~/.bashrc"
alias ll="ls -l"
alias all="ls -al"
alias vup="vagrant up"
alias vhalt="vagrant halt"
alias ag="ag --path-to-ignore ~/.agignore"

# GIT Aliases
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff '
alias gco='git checkout '
alias gp='git pull'
alias gh='git hist'

export REACT_EDITOR=code
